{
    "require": {
        "craftcms/cms": "4.3.10",
        "craftcms/contact-form": "3.0.0",
        "craftcms/contact-form-honeypot": "2.0.0",
        "craftcms/feed-me": "5.0.4",
        "craftcms/redactor": "3.0.3",
        "putyourlightson/craft-sprig": "2.2.1",
        "verbb/field-manager": "3.0.4",
        "verbb/image-resizer": "3.0.2",
        "verbb/knock-knock": "2.0.3",
        "vlucas/phpdotenv": "^5.4.0"
    },
    "require-dev": {
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0.3"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
